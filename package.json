{
  "name": "@qubit-ltd/vite-plugin-babel",
  "version": "1.6.3",
  "description": "A simple vite plugin to support Babel.",
  "author": "Haixing Hu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/vite-plugin-babel.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/vite-plugin-babel/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/vite-plugin-babel",
  "keywords": [
    "vite",
    "vite-plugin",
    "babel"
  ],
  "main": "dist/vite-plugin-babel.cjs",
  "module": "dist/vite-plugin-babel.mjs",
  "exports": {
    ".": {
      "require": "./dist/vite-plugin-babel.cjs",
      "import": "./dist/vite-plugin-babel.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "type": "module",
  "scripts": {
    "clean": "rm -rvf dist",
    "lint": "eslint ./src",
    "build": "node esbuild.mjs",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "deploy": "npm run lint && npm run build && npm publish --registry='https://npm.qubit.ltd/' --access public",
    "deploy:public": "npm run lint && npm run build && npm publish --registry='https://registry.npmjs.org/' --access public",
    "deploy:all": "npm run deploy && npm publish --registry='https://registry.npmjs.org/' --access public"
  },
  "dependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@qubit-ltd/eslint-config": "^1.3.4",
    "esbuild": "^0.24.0",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "vitest": "^1.0.0"
  },
  "packageManager": "yarn@4.12.0"
}
